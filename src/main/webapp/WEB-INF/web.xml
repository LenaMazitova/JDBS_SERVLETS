<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>JDBS_SERVLETS</display-name>
    <welcome-file-list>
      <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
      <servlet-name>AddAthleteServlet</servlet-name>
      <servlet-class>app.AddAthleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>AddAthleteServlet</servlet-name>
      <url-pattern>/add</url-pattern>
    </servlet-mapping> 
    <servlet>
      <servlet-name>AddCoachServlet</servlet-name>
      <servlet-class>app.AddCoachServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>AddCoachServlet</servlet-name>
      <url-pattern>/add_new_coach</url-pattern>
    </servlet-mapping> 
    <servlet>
      <servlet-name>ListServlet</servlet-name>
      <servlet-class>app.ListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>ListServlet</servlet-name>
      <url-pattern>/list</url-pattern>
    </servlet-mapping> 
    <servlet>
      <servlet-name>UpdateAthleteServlet</servlet-name>
      <servlet-class>app.UpdateAthleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>UpdateAthleteServlet</servlet-name>
      <url-pattern>/update</url-pattern>
    </servlet-mapping> 
    <servlet>
      <servlet-name>DeleteAthleteServlet</servlet-name>
      <servlet-class>app.DeleteAthleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>DeleteAthleteServlet</servlet-name>
      <url-pattern>/delete</url-pattern>
    </servlet-mapping> 
</web-app>